{
  "name": "partykit-vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel client:dev server:dev",
    "client:dev": "vite",
    "client:build": "tsc && vite build",
    "client:preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "server:dev": "partykit dev",
    "deploy": "npm run client:build && partykit deploy"
  },
  "dependencies": {
    "lodash.throttle": "^4.1.1",
    "partykit": "^0.0.56",
    "partysocket": "^0.0.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.8",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.2"
  }
}
